{
    "name": "dbs-scrapper",
    "version": "0.1.0",
    "description": "",
    "main": "src/index.js",
    "scripts": {
        "start": "node --unhandled-rejections=strict src/index.js",
        "debug": "node --inspect-brk=0.0.0.0:9229 --unhandled-rejections=strict src/index.js",
        "test": "jest --coverage"
    },
    "jest": {
        "collectCoverageFrom": [
            "**/*.js",
            "!**/*.test.js",
            "!**/*.test.constants.js",
            "!**/node_modules/**"
        ],
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/src/tools/",
            "/src/index.js"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/Xotl/dbs-scraper.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Xotl/dbs-scraper/issues"
    },
    "homepage": "https://github.com/Xotl/dbs-scraper#readme",
    "dependencies": {
        "cheerio": "^1.0.0-rc.2",
        "node-fetch": "^1.7.3"
    },
    "devDependencies": {
        "jest": "^25.1.0"
    }
}
